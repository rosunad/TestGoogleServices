@{
    ViewBag.Title = "Home Page";
}
<div id="placesContainer"></div>

@section scripts{
    <script src="~/Scripts/Highcharts-5.0.9/code/highcharts.js" type="text/javascript"></script>
    <script src="~/Scripts/PlacesGraph/places.graph.js" type="text/javascript"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDVFNHiMcbjPqrbuedlUgU1TnGgAIbjdMw&callback=initMap&libraries=places"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            
        });

        function initMap() {
            
            $("body").placesGraph({
                container: "placesContainer",
                latitude: 37.40,
                longitude: -5.97
            });
        }
    </script>
}